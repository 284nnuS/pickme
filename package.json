{
   "name": "pickme",
   "scripts": {
      "dev": "run-p dev:server dev:java",
      "dev:server": "env-cmd --silent -f configs/nextjs.env ts-node --project server.tsconfig.json server/index.ts",
      "dev:java": "env-cmd --silent -f configs/java.env mvn -f java/pom.xml --define java.net.preferIPv4Stack=true --define thorntail.http.port=3001 thorntail:run",
      "build": "run-p build:server build:next build:java",
      "build:server": "tsc --project server.tsconfig.json",
      "build:next": "next build",
      "build:java": "mvn -f java/pom.xml package",
      "start": "run-p start:server start:java",
      "start:server": "pm2-runtime dist/server/index.js",
      "start:java": "java --define java.net.preferIPv4Stack=true --define thorntail.http.port=3001 -jar java/target/pickme_restful_api-thorntail.jar",
      "test": "run-s test:java",
      "test:java": "env-cmd --silent -f configs/java.env mvn -f java/pom.xml clean test",
      "postinstall": "husky install"
   },
   "dependencies": {
      "@mantine/core": "^3.6.11",
      "@mantine/dates": "^3.6.11",
      "@mantine/dropzone": "^3.6.11",
      "@mantine/hooks": "^3.6.11",
      "@mantine/modals": "^3.6.11",
      "@react-hook/throttle": "^2.2.0",
      "axios": "^0.26.0",
      "classnames": "^2.3.1",
      "cookie-parser": "^1.4.6",
      "dayjs": "^1.10.7",
      "express": "^4.17.3",
      "fuse.js": "^6.5.3",
      "helmet": "^5.0.1",
      "http-proxy-middleware": "^2.0.1",
      "next": "12.1.0",
      "next-auth": "^4.2.1",
      "node-cache": "^5.1.2",
      "react": "^17.0.2",
      "react-dom": "^17.0.2",
      "react-icons": "^4.3.1",
      "react-tinder-card": "^1.4.5",
      "sharp": "^0.30.2",
      "socket.io": "^4.4.1",
      "socket.io-client": "^4.4.1",
      "uuid-by-string": "^3.0.6"
   },
   "devDependencies": {
      "@babel/core": "^7.17.5",
      "@types/cookie-parser": "^1.4.2",
      "@types/express": "^4.17.13",
      "@types/node": "^17.0.18",
      "@types/react": "^17.0.38",
      "@typescript-eslint/eslint-plugin": "^5.12.0",
      "@typescript-eslint/parser": "^5.12.0",
      "autoprefixer": "^10.4.2",
      "cors": "^2.8.5",
      "env-cmd": "^10.1.0",
      "eslint": "8.9.0",
      "eslint-config-next": "12.1.0",
      "eslint-config-prettier": "^8.3.0",
      "husky": "^7.0.4",
      "lint-staged": "^12.1.7",
      "npm-run-all": "^4.1.5",
      "postcss": "^8.4.5",
      "prettier": "^2.5.1",
      "tailwindcss": "^3.0.23",
      "ts-node": "^10.4.0",
      "typescript": "^4.5.5"
   }
}
