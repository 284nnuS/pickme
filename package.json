{
   "name": "pickme",
   "scripts": {
      "dev": "run-p dev:server dev:java",
      "dev:server": "env-cmd --silent -f configs/nextjs.env ts-node --project server.tsconfig.json server/index.ts",
      "dev:java": "env-cmd --silent -f configs/java.env mvn -f java/pom.xml --define java.net.preferIPv4Stack=true --define thorntail.http.port=3001 thorntail:run",
      "build": "run-p build:server build:next build:java",
      "build:server": "tsc --project server.tsconfig.json",
      "build:next": "next build",
      "build:java": "mvn -f java/pom.xml package",
      "start": "run-p start:server start:java",
      "start:server": "NODE_ENV=production pm2-runtime dist/server/index.js",
      "start:java": "java -Djava.net.preferIPv4Stack=true -Dthorntail.http.port=3001 -jar java/target/pickme_restful_api-thorntail.jar",
      "test": "run-s test:java",
      "test:java": "env-cmd --silent -f configs/java.env mvn -f java/pom.xml clean test",
      "postinstall": "husky install"
   },
   "dependencies": {
      "@mantine/core": "^4.0.4",
      "@mantine/dates": "^4.0.4",
      "@mantine/dropzone": "^4.0.4",
      "@mantine/hooks": "^4.0.4",
      "@mantine/modals": "^4.0.4",
      "@mantine/next": "^4.0.4",
      "@mantine/notifications": "^4.0.4",
      "@react-hook/throttle": "^2.2.0",
      "axios": "^0.26.1",
      "classnames": "^2.3.1",
      "cookie-parser": "^1.4.6",
      "dayjs": "^1.10.7",
      "deep-object-diff": "^1.1.7",
      "emoji-picker-react": "^3.5.1",
      "express": "^4.17.3",
      "express-http-proxy": "^1.6.3",
      "fuse.js": "^6.5.3",
      "helmet": "^5.0.1",
      "js-base64": "^3.7.2",
      "next": "12.1.0",
      "next-auth": "^4.2.1",
      "node-cache": "^5.1.2",
      "react": "^17.0.2",
      "react-dom": "^17.0.2",
      "react-icons": "^4.3.1",
      "react-tinder-card": "^1.4.5",
      "react-use": "^17.3.2",
      "socket.io": "^4.4.1",
      "socket.io-client": "^4.4.1",
      "uuid-by-string": "^3.0.6"
   },
   "devDependencies": {
      "@babel/core": "^7.17.5",
      "@types/cookie-parser": "^1.4.2",
      "@types/express": "^4.17.13",
      "@types/express-http-proxy": "^1.6.3",
      "@types/node": "^17.0.18",
      "@types/react": "^17.0.40",
      "@typescript-eslint/eslint-plugin": "^5.14.0",
      "@typescript-eslint/parser": "^5.14.0",
      "autoprefixer": "^10.4.2",
      "cors": "^2.8.5",
      "env-cmd": "^10.1.0",
      "eslint": "8.11.0",
      "eslint-config-next": "12.1.0",
      "eslint-config-prettier": "^8.5.0",
      "husky": "^7.0.4",
      "lint-staged": "^12.3.5",
      "npm-run-all": "^4.1.5",
      "postcss": "^8.4.8",
      "prettier": "^2.5.1",
      "tailwindcss": "^3.0.23",
      "ts-node": "^10.7.0",
      "typescript": "^4.6.2"
   }
}
